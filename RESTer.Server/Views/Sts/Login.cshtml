@{
    ViewData["Title"] = "Login";
}
@model RESTer.Server.Models.Sts.LoginModel

<h2>Login</h2>
<ul>
    <li>
        @for (int i = 0; i < Model.AuthenticationSchemes.Length; i++)
        {
            <a asp-route-idp="@Model.AuthenticationSchemes[i].Name" asp-route-returnUrl="@Model.ReturnUrl">
                @Model.AuthenticationSchemes[i].DisplayName
            </a>
        }
    </li>
</ul>